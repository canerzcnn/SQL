3-
UPDATE employee
SET name = 'Deneme'
WHERE email = 'gshelp1@cafepress.com'

UPDATE employee
SET name='Benny Deal'
WHERE id= '22'

UPDATE employee
SET email = 'rockie@gmail.com'
WHERE id= '6'

UPDATE employee
SET name = 'Clever Boy'
WHERE birthday = '1989-02-16'

UPDATE employee
SET birthday = '1977-11-21'
WHERE id = '33';

